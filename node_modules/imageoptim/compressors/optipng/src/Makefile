CC = gcc

LD = $(CC)


all:
	${MAKE} -f scripts/gcc.mak \
	  CC="${CC}" LD="${LD}" 

install:
	${MAKE} -f scripts/gcc.mak install \
	  CC="${CC}" LD="${LD}" 

uninstall:
	${MAKE} -f scripts/gcc.mak uninstall \
	  CC="${CC}" LD="${LD}" 

clean:
	${MAKE} -f scripts/gcc.mak clean \
	  CC="${CC}" LD="${LD}" 
